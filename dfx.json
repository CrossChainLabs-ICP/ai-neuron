{
  "canisters": {
    "ai-neuron-backend": {
      "main": "src/ai-neuron-backend/main.mo",
      "type": "motoko"
    },
    "ai-neuron-backend-worker": {
      "type": "motoko",
      "main": "src/ai-neuron-backend/worker.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "ai-neuron-frontend": {
      "dependencies": [
        "ai-neuron-backend"
      ],
      "source": [
        "src/ai-neuron-frontend/dist"
      ],
      "type": "assets",
      "workspace": "ai-neuron-frontend"
    },
    "ai-neuron-ocbot": {
      "main": "src/ai-neuron-ocbot/main.mo",
      "type": "motoko"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}